{
  "manifest_version": 2,
  "name": "Keytar",
  "description": "Description",
  "version": "0.1",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "keyboard.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
