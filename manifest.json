{
  "manifest_version": 2,
  "name": "Keytar",
  "description": "Description",
  "version": "0.1",
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [ "style.css" ],
      "js": ["jquery-2.1.4.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["jquery-2.1.4.min.js"],
    "persistent": false
  }
}
